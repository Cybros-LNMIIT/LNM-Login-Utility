{
    "manifest_version": 2,
    "name": "LNM Login Utility",
    "description": "An extention made by a frustrated developer to ease off the login functionality of the portal",
    "homepage_url": "http://chaitanyagiri.github.io/",
    "version": "1.0",
    "content_scripts": [
        { 
        "matches": ["*://172.22.2.6/connect/*"],
        "js": ["myscript.js"]
        }
        ],
    "browser_action": {
        "default_title": "Click here!",
        "default_popup": "index.html"
    },
    "permissions": [
        "tabs",
        "*://172.22.2.6/connect/*"
    ]
}
